{
  "name": "csv-ts",
  "version": "0.1.1",
  "description": "TSV/CSV Embed in YAML",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:common": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "jest",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint:prettier": "prettier --write .",
    "prepare": "if type simple-git-hooks; then simple-git-hooks; fi",
    "prepublishOnly": "run-p build:*"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint"
  },
  "bin": {
    "my-lib": "dist/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masaharu-kato/csv-ts.git"
  },
  "keywords": [
    "TSV",
    "CSV",
    "typescript",
    "parser",
    "data"
  ],
  "author": "Masaharu Kato",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/masaharu-kato/csv-ts/issues"
  },
  "homepage": "https://github.com/masaharu-kato/csv-ts#readme",
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "csv": "^6.2.3",
    "csv-parse": "^5.3.2"
  }
}
